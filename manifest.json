{
  "name": "Fingerprint Collector",
  "version": "0.1",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}